generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model User {
    id       Int    @id @unique @default(autoincrement())
    email    String @unique
    password String
}

model Vehicle {
    id        Int     @id @unique @default(autoincrement())
    type      String
    brand     String
    model     String
    available Boolean @default(true)
}
